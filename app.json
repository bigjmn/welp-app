{
  "expo": {
    "name": "welp",
    "slug": "welp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/welplogos/welpwicon.png",
    "scheme": "welp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true,
      "bundleIdentifier": "com.bigjmn.welp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "expo-build-properties",
        {
          "ios": {
            "use-frameworks": "static"
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/MobileRoboto/Roboto-Regular.ttf",
            "./assets/fonts/MobileRoboto/Roboto-Bold.ttf",
            "./assets/fonts/MobileRoboto/Roboto-Light.ttf",
            "./assets/fonts/MobileRoboto/Roboto-LightItalic.ttf",
            "./assets/fonts/MobileRoboto/Roboto-Medium.ttf",
            "./assets/fonts/UberMove-Medium.ttf"
          ]
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/welplogos/welp_white_cutout.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#8B5CF6",
          "dark": {
            "backgroundColor": "#8B5CF6"
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Welp to use your location."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0ce3bbb4-579b-4655-a2b8-0ae2f44d2097"
      }
    },
    "owner": "bigjmn",
    "android": {
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.bigjmn.welp"
    }
  }
}
